{
  "name": "geins-sdk",
  "version": "0.0.5",
  "author": "Geins",
  "private": true,
  "license": "MIT",
  "type": "module",
  "workspaces": [
    "packages/config/*",
    "packages/sdk/*"
  ],
  "scripts": {
    "build": "yarn codegen &&  turbo build",
    "dev": "turbo dev",
    "dev:nuxt": "turbo dev --filter=nuxt-app",
    "start:nuxt": "turbo start --filter=nuxt-app",
    "build:nuxt": "turbo build --filter=nuxt-app",
    "build:sdk": "turbo build --filter=./packages/sdk/*",
    "lint": "turbo lint",
    "link": "turbo run link",
    "format": "prettier --write \"**/*.{ts,md,vue,js}\"",
    "prepare": "husky",
    "clean": "node scripts/clean.cjs",
    "codegen": "yarn workspace @geins/types codegen",
    "prepare-schema": "node scripts/prepare-schema.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "changeset-add": "changeset add",
    "version:next": "changeset version --no-git-tag --snapshot canary",
    "release:next": "changeset version --no-git-tag --snapshot canary && changeset publish --no-git-tag --tag canary",
    "version": "changeset version",
    "release": "changeset version && changeset publish"
  },
  "lint-staged": {
    "*.{ts,md,vue,js}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "devDependencies": {
    "@algolia/client-search": "^5.13.0",
    "@babel/core": "^7.26.0",
    "@changesets/cli": "^2.27.9",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/travis-cli": "^19.5.0",
    "@graphql-tools/jest-transform": "^2.0.0",
    "@mermaid-js/mermaid-mindmap": "^9.3.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.9.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.14.0",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-transform-graphql": "^2.1.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^15.2.10",
    "mermaid": "^11.3.0",
    "prettier": "^3.3.3",
    "prettier-plugin-vue": "^1.1.6",
    "search-insights": "^2.17.2",
    "ts-jest": "^29.2.5",
    "turbo": "^2.1.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.11",
    "vitepress": "^1.3.4",
    "vitepress-plugin-mermaid": "^2.0.17"
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "cross-fetch": "^4.0.0",
    "jest-environment-jsdom-fourteen": "^1.0.1"
  }
}